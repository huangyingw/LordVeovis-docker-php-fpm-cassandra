[remote "origin"]
    url = git@github.com:LordVeovis/docker-php-fpm-cassandra.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[remote "github"]
    url = git@github.com:huangyingw/LordVeovis-docker-php-fpm-cassandra.git
    fetch = +refs/heads/*:refs/remotes/github/*
[push]
    remote = github
[gsync]
    remote = origin
    branch = master
[merge]
	allow = true
[branch "dev"]
    remote = github
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[commit]
	message = n
[gdif]
    ignore = *.gdio.diff
[vrun]
	silent = false
	async = false
[deploy]
	host = localhost
	path = ~/myproject/git/php/LordVeovis-docker-php-fpm-cassandra
